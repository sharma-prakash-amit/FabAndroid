<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context="com.fab.kh2280.fab.ContinueAsGuestOrAdminLogin">

    <Button
        android:id="@+id/btnAdminLogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btnGuestContinue"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="51dp"
        android:layout_marginEnd="113dp"
        android:text="@string/AdminLogin"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="149dp" />

    <Button
        android:id="@+id/btnGuestContinue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/btnAdminLogin"
        android:layout_marginBottom="184dp"
        android:text="@string/GuestContinue"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="269dp" />
</RelativeLayout>
